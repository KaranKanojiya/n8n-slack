# Postgres (used by both postgres service and n8n)
POSTGRES_USER=n8n
POSTGRES_PASSWORD=change_me_secure_password
POSTGRES_DB=n8n

# n8n encryption (required when using Postgres; generate with: openssl rand -hex 32)
N8N_ENCRYPTION_KEY=your_32_char_hex_encryption_key

# n8n Basic Auth (recommended for local exposure via ngrok)
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=change_me_admin_password

# Public URL for webhooks (set to your ngrok https URL after running: ngrok http 5678)
# Example: https://abc123.ngrok-free.app
WEBHOOK_URL=
